{
    "workbench.startupEditor": "none",
    "go.toolsManagement.autoUpdate": true,
    "editor.wordWrap": "on",
    "gitlab.featureFlags.languageServer": true,
    "git.confirmSync": false,
    "files.autoSave": "afterDelay",
    "terminal.integrated.fontFamily": "meslolgs nf",
    "git.enableSmartCommit": true,
    "window.zoomLevel": 1,
    "gitlab.ls.debug": true,
    "gitlab.duoCodeSuggestions.preferredAccount": "dnsmichi",
    "gitlab.duoCodeSuggestions.additionalLanguages": [
        "powershell",
        "yaml",
        "ansible",
        "cobol",
        "abap",
        "perl",
        "dockerfile",
        "markdown",
        "vb",
        "json"
    ],
    "gitlab.duoAgenticChat.enabled": true,
    "gitlab.featureFlags.agenticTabs": true,
    "gitlab.duoWorkflow.useDocker": false,
    "gitlab.featureFlags.agenticDuoChat": true,
    "gitlab.duo.workflow.executor": "node",
    "workbench.settings.applyToAllProfiles": [
        "gitlab.duoChat.enabled"
    ],
    "print.folder.fileNames": true,
    "print.folder.include": [],
    "print.folder.exclude": [
        "{bin,obj,out}",
        "node_modules",
        "data",
        "**/*.{bin,exe,dll,hex,pdb,pdf,pfx,png,jpg,gif,bmp,suo,design}"
    ],
    "print.folder.maxLines": 1200,
    "print.folder.maxFiles": 30,
    "print.folder.includeFileList": false,
    "files.trimTrailingWhitespace": true,
    "terminal.external.osxExec": "Iterm2.app",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.fontSize": 14,
    "explorer.confirmDelete": false,
    "gitlab.debug": true,
    "workbench.editor.empty.hint": "hidden",
    "chat.agent.enabled": false,
    "chat.extensionTools.enabled": false,
    "chat.mcp.enabled": false,
    "telemetry.feedback.enabled": false,
    "chat.commandCenter.enabled": false,
    "chat.edits2.enabled": false,
}